{
  "name": "solvrn-sdk",
  "version": "1.0.8",
  "description": "Privacy SDK for Solana governance using zero-knowledge proofs and confidential computing",
  "type": "module",
  "keywords": [
    "solana",
    "governance",
    "zk",
    "zero-knowledge",
    "privacy",
    "voting",
    "noir",
    "arcium",
    "mpc",
    "blockchain"
  ],
  "author": "SOLVRN Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrhoDev/SVRN"
  },
  "bugs": {
    "url": "https://github.com/OrhoDev/SVRN/issues"
  },
  "homepage": "https://github.com/OrhoDev/SVRN/tree/main/sdk#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/index.js": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "@arcium-hq/client": "^0.6.2",
    "@aztec/bb.js": "^3.0.0-nightly.20260102",
    "@coral-xyz/anchor": "^0.32.1",
    "@noir-lang/noir_js": "1.0.0-beta.15",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.95.0",
    "bn.js": "^5.2.2",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
